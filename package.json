{
  "name": "move-box",
  "version": "0.0.1",
  "description": "Webcomponent move-box following open-wc recommendations",
  "license": "MIT",
  "author": "move-box",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint --ext .js,.html . --fix --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.js\" --write --ignore-path .gitignore",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint --ext .js,.html . --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.js\" --check --ignore-path .gitignore",
    "start": "es-dev-server --app-index demo/index.html --node-resolve --open --watch",
    "storybook": "start-storybook --node-resolve --watch --open",
    "storybook:build": "build-storybook",
    "test": "karma start --coverage",
    "test:bs": "karma start karma.bs.config.js --coverage",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:watch": "karma start --auto-watch=true --single-run=false"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/package.json": [
      "sort-package-json"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "prettier": "@open-wc/prettier-config",
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@open-wc/demoing-storybook": "^1.10.5",
    "@open-wc/eslint-config": "^1.0.0",
    "@open-wc/prettier-config": "^0.1.14",
    "@open-wc/testing": "^2.5.4",
    "@open-wc/testing-karma": "^3.2.43",
    "@open-wc/testing-karma-bs": "^1.3.43",
    "es-dev-server": "^1.40.1",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "sort-package-json": "^1.40.0",
    "webpack-merge": "^4.1.5"
  }
}
